{
  "name": "joke-api",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "test": "yarn run jasmine",
    "start": "node --require esm src/index.js",
    "dev:test": "yarn run nodemon --ext \"js,json,yaml,yaml\" --exec \"yarn test\"",
    "dev:start": "yarn run nodemon --ext \"js,json,yaml,yaml\" --exec \"yarn start\"",
    "dev": "yarn run nodemon --ext \"js,json,yaml,yaml\" --exec \"yarn test && yarn start\""
  },
  "nodemonConfig": {
    "ignore": [ "node_modules/*", ".git/*" ],
    "ext": "js,json,yml,yaml",
    "delay": "500"
  },
  "devDependencies": {
    "jasmine": "^3.3.1",
    "nodemon": "^1.18.10",
    "sequelize-cli": "^5.4.0",
    "supertest": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "esm": "^3.2.14",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "pg": "^7.8.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.15.1",
    "swagger-jsdoc": "^3.2.7",
    "swagger-tools": "^0.10.4",
    "yamljs": "^0.3.0"
  }
}
